{
  "family": "discodeit-task",
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::737075001387:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::737075001387:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "discodeit-app",
      "image": "public.ecr.aws/q2u8q8z8/codeit/discodeit:latest",
      "cpu": 256,
      "memory": 512,
      "memoryReservation": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::discodeit-binary-content-storage-lwg/discodeit.env",
          "type": "s3"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/discodeit-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-create-group": "true",
          "awslogs-stream-prefix": "ecs",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      }
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
